{
  "name": "thingful-server",
  "version": "0.0.1",
  "description": "Starter code for learning about authentication.",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "prettier-watch": "onchange \"**/*.js\" -- prettier --write {{changed}}",
    "test": "mocha --require test/setup.js --watch",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "start": "node src/server.js",
    "predeploy": "npm audit",
    "deploy": "git push heroku master"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "helmet": "^3.21.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "treeize": "^2.1.2",
    "xss": "^1.0.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "onchange": "^6.1.0",
    "postgrator-cli": "^3.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "supertest": "^3.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "contributors": [
    "Thomas `tomatao` Hudspith-Tatham <iammotivated@gmail.com>",
    "Joshua Reade Harris <jharris@thinkful.com>"
  ],
  "license": "MIT"
}
